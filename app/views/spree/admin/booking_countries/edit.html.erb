<h1>Editing booking_country</h1>

<%= semantic_form_for([:admin,@booking_country]) do |f| %>

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :group_id %><br />
    <%= f.collection_select :group_id,@booking_groups,:id,:name %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>


<%= link_to 'Show', admin_booking_country_path(@booking_country) %> |
<%= link_to 'Back', admin_booking_countries_path %>
